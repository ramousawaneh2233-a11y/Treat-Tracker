generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // We removed the 'url' line from here because Prisma 7 requires it in the config
}

model User {
  id            String    @id @default(cuid())
  name          String?
  email         String?   @unique
  password      String?
  image         String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}
